{
  "name": "@general/ui",
  "version": "1.6.3-beta.1",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "release": "semantic-release -e semantic-release-monorepo"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:forforeach/lerna-poc.git"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "7.0.0-beta.2",
    "@semantic-release/error": "2.2.0",
    "@semantic-release/git": "7.1.0-beta.3",
    "@semantic-release/github": "5.3.0-beta.8",
    "@semantic-release/npm": "5.2.0-beta.6",
    "@semantic-release/release-notes-generator": "7.1.6",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "semantic-release": "16.0.0-beta.18",
    "semantic-release-monorepo": "^6.1.1",
    "semantic-release-plugin-decorators": "^2.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "+([1-9])?(.{+([1-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "ci": false,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "gitHead": "e4e9c4374911daa22c91104a4eeb145fe2fe8e5c"
}
